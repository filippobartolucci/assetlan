asset z;

int fibonacci(int n)[asset a]{
    if (n == 0) {
        transfer a;
        return 0;
    }
    if (n == 1){
        a -o z;
        return 1;
    }
    a -o z;
    return fibonacci(n - 1)[z] + fibonacci(n - 2)[z];
}


void main ()[asset a]{
    int n;
    a -o z;

    n = fibonacci(3)[z];

}
main()[10];





