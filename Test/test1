int a = 1; // a -> int, 1
asset b; // b -> asset, 2

// g -> int
int g(int a)[]{  // a -> int, 1
    if (a == 0) {

    }
}

// f -> void
void f(int pippetta)[]{ // pippetta -> int, 1
    print g(pippetta)[];
}

void main(int x)[asset z]{ // x -> int, 1 z -> asset, 2
    print z;
    f(10)[];
    transfer z;
}

main(1)[10];